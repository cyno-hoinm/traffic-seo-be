  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^5.1.0",
    "nodemon": "^3.1.9",
    "pg": "^8.14.1",
    "qs": "^6.14.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
